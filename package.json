    "@vercel/og": "^0.8.6",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "katex": "^0.16.28",
    "mime-types": "^3.0.2",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "next-view-transitions": "^0.3.5",
    "nextra": "^4.6.1",
    "nextra-theme-blog": "^4.6.1",
    "react": "^19.2.4",
    "react-devicons": "^2.16.2",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.1",
    "theme-change": "^2.5.0",
    "yargs": "^18.0.0"
