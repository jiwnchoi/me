    "@types/js-yaml": "^4.0.9",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.18",
    "eslint": "^10.0.0",
    "eslint-config-next": "16.1.6",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.1",
    "lint-staged": "^16.2.7",
    "pagefind": "^1.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "quicktype-core": "^23.2.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
